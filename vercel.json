{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://e-commerce-admin-six-vert.vercel.app, http://localhost:3000",
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,PATCH,OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With, Accept, Origin"
      }
    },
    {
      "src": "/admin/(.*)",
      "dest": "/src/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://e-commerce-admin-six-vert.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}